{
  "build": {
    "development": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.radyoklasik.online",
        "EXPO_PUBLIC_SHARED_SECRET": "${EXPO_PUBLIC_SHARED_SECRET}"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.radyoklasik.online",
        "EXPO_PUBLIC_SHARED_SECRET": "${EXPO_PUBLIC_SHARED_SECRET}"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.radyoklasik.online",
        "EXPO_PUBLIC_SHARED_SECRET": "${EXPO_PUBLIC_SHARED_SECRET}"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "cli": {
    "promptToConfigurePushNotifications": false
  }
}
